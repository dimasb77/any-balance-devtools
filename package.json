{
  "name": "any-balance-devtools",
  "version": "1.2.5",
  "main": "./build/app/app.js",
  "bin": "./build/app/app.js",
  "license": "MIT",
  "devDependencies": {
    "@types/app-root-path": "^1.2.4",
    "@types/archiver": "^5.1.0",
    "@types/body-parser": "^1.19.0",
    "@types/express": "^4.17.11",
    "@types/fs-extra": "^9.0.13",
    "@types/google-closure-compiler": "^0.0.18",
    "@types/got": "^9.6.11",
    "@types/http-errors": "^1.8.0",
    "@types/node": "^16.10.1",
    "@types/shelljs": "^0.8.8",
    "@types/uuid": "^8.3.0",
    "@types/vhost": "^3.0.2",
    "@types/yargs": "^17.0.0",
    "pkg": "^5.3.0",
    "run-script-os": "^1.1.6",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.4.3"
  },
  "dependencies": {
    "app-root-path": "^3.0.0",
    "archiver": "^5.3.0",
    "body-parser": "^1.19.0",
    "express": "^4.17.1",
    "fontoxpath": "^3.18.1",
    "fs-extra": "^10.0.0",
    "git-auto-update": "https://github.com/dukei/git-auto-update.git#master",
    "google-closure-compiler": "^20210601.0.0",
    "got": "^11.8.0",
    "hpagent": "^0.1.1",
    "http-errors": "^1.8.0",
    "log4js": "^6.3.0",
    "node-schematron": "^2.0.1",
    "puppeteer": "^10.0.0",
    "readline-promise": "^1.0.5",
    "shelljs": "^0.8.4",
    "socks-proxy-agent": "^5.0.0",
    "twing": "^5.0.2",
    "uuid": "^8.3.2",
    "xmldom-ts": "^0.3.1",
    "xmllint-wasm": "^2.0.0",
    "yargs": "^17.0.1"
  },
  "scripts": {
    "tsc": "tsc",
    "build-compress": "yarn build --compress Brotli",
    "build": "run-script-os",
    "build:default": "tsc && pkg .",
    "postbuild": "run-script-os",
    "postbuild:win32": "postbuild.bat",
    "postbuild:default": "echo postbuild step is not yet implemented for this os",
    "dev": "ts-node-dev --respawn --transpile-only ./app/app.ts",
    "devt": "ts-node-dev --respawn --transpile-only ./app/test.ts",
    "debug": "tsc && node --inspect-brk ./build/app/app.js",
    "debugt": "tsc && node --inspect-brk ./build/app/test.js"
  },
  "pkg": {
    "scripts": "build/**/*.js",
    "assets": "res/**/*",
    "targets": [
      "node12-win-x64"
    ],
    "outputPath": "abd"
  }
}
